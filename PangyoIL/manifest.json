{
  "manifest_version": 3,
  "name": "pangyotranslator",
  "version": "1.0",
  "description": "판교어를 해독해주고, 변환하는 확장프로그램입니다!!",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "pangyotranslator",
    "default_icon": {
      "16": "static/pangyotranslator.png",
      "32": "static/pangyotranslator.png",
      "48": "static/pangyotranslator.png",
      "128": "static/pangyotranslator.png"
    }
  },
  "icons": {
    "16": "static/pangyotranslator.png",
    "32": "static/pangyotranslator.png",
    "48": "static/pangyotranslator.png",
    "128": "static/pangyotranslator.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}