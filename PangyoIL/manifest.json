{
  "manifest_version": 3,
  "name": "pangyotranslator",
  "version": "1.0",
  "description": "판교어를 해독해주고, 변환하는 확장프로그램입니다!!",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "commands": {
    "translate-to-korean": {
      "suggested_key": {
        "default": "Ctrl+Shift+K"
      },
      "description": "판교어를 한국어로 번역"
    },
    "translate-to-pangyo": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      },
      "description": "한국어를 판교어로 번역"
    }
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "pangyotranslator",
    "default_icon": {
      "16": "static/pangyotranslator.png",
      "32": "static/pangyotranslator.png",
      "48": "static/pangyotranslator.png",
      "128": "static/pangyotranslator.png"
    }
  },
  "icons": {
    "16": "static/pangyotranslator.png",
    "32": "static/pangyotranslator.png",
    "48": "static/pangyotranslator.png",
    "128": "static/pangyotranslator.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}